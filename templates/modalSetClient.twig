<div class="modal fade" id="modalSetClientByButton" tabindex="-1" role="dialog">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			<div class="modal-header background-template-color">
				<span class="modal-title  text-template-color">Clientes</span>
				{# <button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button> #}
			</div>
			<div class="modal-body">
				<div class="input-group shadow-sm mb-1">
					<input 
						type="text" 
						list="listClient_SetClientByButton" 
						id="inputTextToSearchClient_SetClientByButton" 
						class="form-control form-control-sm text-center"  
						title="Ingrese nombre, RUT o CI para buscar cliente" 
						onchange="searchCompleteData(this.value, event, 'listClient_SetClientByButton', 'modalSetClientByButton')" 
						onkeyup="searchClient(this, event, 'listClient_SetClientByButton', 'modalSetClientByButton')" 
						onkeypress="return keyPressAddClient(event, this.value, 100)" 
						autocomplete="off"
					>
					<datalist id="listClient_SetClientByButton" ></datalist>
				</div>
				<div class="d-flex ">
					<div class="form-group w-100 mb-1">
						<label for="inputNameClient_SetClientByButton" class="col-form-label col-form-label-sm obligatorio">Nombre</label>
						<input type="text" class="form-control form-control-sm shadow-sm" id="inputNameClient_SetClientByButton" onkeypress="return keyPressAddClient(event, this.value, 50)">
					</div>
				</div>
				<div class="form-row mb-0">
					<div class="form-group col mb-1">
						<label for="inputDocumentClient_SetClientByButton" class="col-form-label col-form-label-sm obligatorio">Documento</label>
						<input type="number" class="form-control form-control-sm shadow-sm" id="inputDocumentClient_SetClientByButton" onkeypress="return keyPressAddClient(event, this.value, 12)">
					</div>
					<div class="form-group col mb-1">
						<label for="inputPhoneClient_SetClientByButton" class="col-form-label col-form-label-sm">Celular/Teléfono</label>
						<input type="number" class="form-control form-control-sm shadow-sm" id="inputPhoneClient_SetClientByButton" onkeypress="return keyPressAddClient(event, this.value, 12)">
					</div>
				</div>
				<div class="d-flex ">
					<div class="form-group w-100 mb-1">
						<label for="inputEmailClient_SetClientByButton" class="col-form-label col-form-label-sm">Email</label>
						<input type="text" class="form-control form-control-sm shadow-sm" id="inputEmailClient_SetClientByButton" onkeypress="return keyPressAddClient(event, this.value, 280)">
					</div>
				</div>
				<div class="d-flex ">
					<div class="form-group w-100 mb-1">
						<label for="inputAddressClient_SetClientByButton" class="col-form-label col-form-label-sm obligatorio">Dirección</label>
						<input type="text" class="form-control form-control-sm shadow-sm" id="inputAddressClient_SetClientByButton" onkeypress="return keyPressAddClient(event, this.value, 280)">
					</div>
				</div>
				<div class="form-row mb-0">
					<div class="form-group col mb-1">
						<label for="inputCityClient_SetClientByButton" class="col-form-label col-form-label-sm obligatorio">Ciudad</label>
						<input type="text" class="form-control form-control-sm shadow-sm" id="inputCityClient_SetClientByButton" onkeypress="return keyPressAddClient(event, this.value, 50)">
					</div>
					<div class="form-group col mb-1">
						<label for="inputDepartmentClient_SetClientByButton" class="col-form-label col-form-label-sm obligatorio">Departamento</label>
						<input type="text" class="form-control form-control-sm shadow-sm" id="inputDepartmentClient_SetClientByButton" onkeypress="return keyPressAddClient(event, this.value, 50)">
					</div>
				</div>
				<div class="alert alert-info m-0" role="alert">
					( * ): Campo obligatorio
				</div>
			</div>
			<div class="modal-footer justify-content-between">
				<button style="height: 41px;" type="button" class="btn btn-danger shadow-sm" data-dismiss="modal">Cancelar</button>
				<button type="button" class="btn background-template-color2 text-template-background shadow-sm" id="setConsumidorFinalSetClientByButton" onclick="setConsumidorFinalByButton()" data-dismiss="modal" title="Consumidor Final">
					<span class="maintext"> <i class="fas fa-user-times"></i> </span>  <span class="shortcut"> (Esc) </span>
				</button>
				<button style="height: 41px;" type="button" class="btn background-template-color2 text-template-background shadow-sm" onclick="cleanFields('modalSetClientByButton')" title="Limpiar campos"><i class="fas fa-eraser"></i></button>
				<button style="height: 41px;" id="btnConfirmSetClientByButton" onclick="setClient('modalSetClientByButton')" type="button" class="btn btn-success shadow-sm" onkeypress="return keyPressAddClient(event, null, null)">Confirmar</button>
			</div>
		</div>
	</div>
</div>

